{
  "version": 2,
  "routes": [
    {
      "src": "/v1/docs/swagger-ui/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/v1/docs/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/v1/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/health",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 60,
      "includeFiles": "src/docs/**"
    }
  },
  "build": {
    "env": {
      "SHARP_IGNORE_GLOBALLY": "true",
      "NODE_ENV": "production"
    }
  },
  "headers": [
    {
      "source": "/v1/docs/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/v1/docs/json",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ]
}